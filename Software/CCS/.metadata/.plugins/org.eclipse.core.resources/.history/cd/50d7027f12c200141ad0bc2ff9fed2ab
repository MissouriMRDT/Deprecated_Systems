 *  Every name in this file should correspond to a net in the
 *  board schematic
 */

#ifndef ROVEPINMAP_H_
#define ROVEPINMAP_H_

#define HIGH 1
#define LOW 0

//Mux pins
/*
 * Mux pins:
 * U3_MUX_S0 PE0
 * U3_MUX_S1 PE1
 * U4_MUX_S0 PH0
 * U4_MUX_S1 PH1
 * U5_MUX_S0 PE0
 * U5_MUX_S1 PE1
 */


//Uarts

/*
 * Uart pins:
 * U3RX PA4
 * U3TX PA5
 * U2RX PA6
 * U2TX PA7
 * U6RX PK0
 * U6TX PK1
 * U7RX PP0
 * U7TX PP1
 */


#define U3RX PA4
#define U3TX PA5
#define U2RX PA6
#define U2TX PA7
#define U6RX PK0
#define U6TX PK1
#define U7RX PP0
#define U7TX PP1




#endif /* ROVEPINMAP_H_ */
